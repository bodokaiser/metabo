# 6 "/opt/xwinnmr/exp/stan/nmr/lists/pp/zgpg" 2

# 1 "/opt/xwinnmr/exp/stan/nmr/lists/pp/Delay.incl" 1
;Delay.incl  -  include file for commonly used delays
;
;version 00/02/07


;general delays

define delay DELTA
define delay DELTA1
define delay DELTA2
define delay DELTA3
define delay DELTA4
define delay DELTA5
define delay DELTA6
define delay DELTA7
define delay DELTA8

define delay TAU
define delay TAU1
define delay TAU2
define delay TAU3
define delay TAU4
define delay TAU5


;delays for centering pulses

define delay CEN_HN1
define delay CEN_HN2
define delay CEN_HN3
define delay CEN_HC1
define delay CEN_HC2
define delay CEN_HC3
define delay CEN_HC4
define delay CEN_HP1
define delay CEN_HP2
define delay CEN_CN1
define delay CEN_CN2
define delay CEN_CN3
define delay CEN_CN4
define delay CEN_CP1
define delay CEN_CP2


;loop counters

define loopcounter COUNTER
define loopcounter SCALEF
define loopcounter FACTOR1
define loopcounter FACTOR2
define loopcounter FACTOR3



;$Id: Delay.incl,v 1.11 2002/06/12 09:04:22 ber Exp $
# 7 "/opt/xwinnmr/exp/stan/nmr/lists/pp/zgpg" 2




"d11=30m"


"DELTA=d1-100m"


# 1 "mc_line 17 file /opt/xwinnmr/exp/stan/nmr/lists/pp/zgpg expanding definition part of mc command before ze"
define delay MCWRK
define delay MCREST
"MCWRK = 0.500000*30m"
"MCREST = 30m - 30m"
# 17 "/opt/xwinnmr/exp/stan/nmr/lists/pp/zgpg"
1 ze
# 1 "mc_line 17 file /opt/xwinnmr/exp/stan/nmr/lists/pp/zgpg expanding definition of mc command after ze"
# 18 "/opt/xwinnmr/exp/stan/nmr/lists/pp/zgpg"
  d11 pl12:f2
# 1 "mc_line 19 file /opt/xwinnmr/exp/stan/nmr/lists/pp/zgpg expanding start label for mc command"
2 MCWRK  * 2 do:f2
LBLF0, MCREST
# 20 "/opt/xwinnmr/exp/stan/nmr/lists/pp/zgpg"
  10u pl13:f2
  d11 cpd2:f2
  DELTA
  4u do:f2
  10u pl12:f2
  100m cpd2:f2
  p1 ph1
  go=2 ph31
# 1 "mc_line 28 file /opt/xwinnmr/exp/stan/nmr/lists/pp/zgpg expanding mc command in line"
  MCWRK do:f2 pl13:f2 wr #0
  MCWRK zd 
  lo to LBLF0 times td0

# 29 "/opt/xwinnmr/exp/stan/nmr/lists/pp/zgpg"
exit


ph1=0 2 2 0 1 3 3 1
ph31=0 2 2 0 1 3 3 1


;pl1 : f1 channel - power level for pulse (default)
;pl12: f2 channel - power level for CPD/BB decoupling
;pl13: f2 channel - power level for second CPD/BB decoupling
;p1 : f1 channel -  high power pulse
;d1 : relaxation delay; 1-5 * T1
;d11: delay for disk I/O                             [30 msec]
;NS: 1 * n, total number of scans: NS * TD0
;cpd2: decoupling according to sequence defined by cpdprg2
;pcpd2: f2 channel - 90 degree pulse for decoupling sequence



;$Id: zgpg,v 1.7.2.1 2003/04/17 16:18:26 ber Exp $
